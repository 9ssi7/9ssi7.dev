---
import SectionList from "@/components/SectionList.astro";
import { CertificateItems } from "@/data/certificates";
import { useI18n } from "@/i18n/utils";

type Props = {
  class?: string;
};

const { class: className } = Astro.props;

const { t } = useI18n(Astro.locals.lang);
---

<SectionList 
  title={t("category_certificate")}
  items={CertificateItems}
  color="purple"
  icon="award"
  class={className}
/> 